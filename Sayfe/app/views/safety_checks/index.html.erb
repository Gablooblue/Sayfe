<p id="notice"><%= notice %></p>

<h1>Safety Checks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @safety_checks.each do |safety_check| %>
      <tr>
        <td><%= link_to 'Show', safety_check %></td>
        <td><%= link_to 'Destroy', safety_check, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Safety Check', new_safety_check_path %>
