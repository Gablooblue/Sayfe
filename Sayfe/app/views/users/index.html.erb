
<%= render 'layouts/search' %>

<div class = "col-md-12">
<div class = "panel panel-default">
    <h3 class = "text-center">Results</h4>
    <% if @results.blank? %>
	<div class = 'panel-body'>
	    <h4 class = "text-center">No users found</h5>
	</div>
    <% else %>
	<%= react_component "UsersContainer",
	    usersPath: users_path() %>
    <% end %>
</div>
</div>
